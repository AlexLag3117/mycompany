MODULE SalesLedgerReport;

REQUIRE SalesLedger, SalesSettings;

NAMESPACE Sales;

FORM reportSalesLedgers 'Отчет по регистрам продаж'
    OBJECTS salesLedger = SalesLedger
    PROPERTIES(salesLedger) READONLY class, type, dateTime, number, nameItem, nameCustomer,
                           quantity, price, untaxedAmount, taxes, amount
;

NAVIGATOR {
    sales {
            reports {
            NEW reportSalesLedgers;        
        }
    }
}